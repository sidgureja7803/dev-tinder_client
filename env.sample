# ===========================================
# PAYMENT CONFIGURATION
# ===========================================
VITE_RAZORPAY_KEY_ID=rzp_test_your_key_id_here
# This is the publishable key, not the secret

# ===========================================
# ENVIRONMENT CONFIGURATION
# ===========================================
VITE_NODE_ENV=development
# VITE_NODE_ENV=production (for production build)

VITE_APP_VERSION=1.0.0
VITE_APP_NAME=DevTinder

# ===========================================
# FEATURE FLAGS
# ===========================================
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_CHAT=true
VITE_ENABLE_PREMIUM_FEATURES=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_GEOLOCATION=true

# ===========================================
# THIRD-PARTY INTEGRATIONS
# ===========================================
# Google Analytics
VITE_GA_TRACKING_ID=G-XXXXXXXXXX

# Sentry (Error Tracking)
VITE_SENTRY_DSN=your_sentry_dsn_for_frontend_error_tracking

# Hotjar (User Analytics)
VITE_HOTJAR_ID=your_hotjar_id

# Crisp Chat (Customer Support)
VITE_CRISP_WEBSITE_ID=your_crisp_website_id

# ===========================================
# SOCIAL MEDIA CONFIGURATIONS
# ===========================================
VITE_FACEBOOK_APP_ID=your_facebook_app_id
VITE_GOOGLE_CLIENT_ID=your_google_oauth_client_id
VITE_LINKEDIN_CLIENT_ID=your_linkedin_oauth_client_id
VITE_GITHUB_CLIENT_ID=your_github_oauth_client_id

# ===========================================
# MAP INTEGRATION (Optional)
# ===========================================
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token

# ===========================================
# FILE UPLOAD CONFIGURATION
# ===========================================
VITE_MAX_FILE_SIZE=5242880
# 5MB in bytes (5 * 1024 * 1024)
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp
VITE_MAX_PHOTOS_COUNT=5

# ===========================================
# UI/UX CONFIGURATION
# ===========================================
VITE_DEFAULT_THEME=dark
# Options: light, dark, system
VITE_ENABLE_ANIMATIONS=true
VITE_ANIMATION_DURATION=300
# Animation duration in milliseconds

# ===========================================
# DEVELOPMENT CONFIGURATION
# ===========================================
VITE_DEBUG_MODE=true
# Enable/disable debug logs
VITE_MOCK_API=false
# Use mock API for development
VITE_ENABLE_REDUX_DEVTOOLS=true

# ===========================================
# PWA CONFIGURATION
# ===========================================
VITE_ENABLE_PWA=true
VITE_PWA_SHORT_NAME=DevTinder
VITE_PWA_DESCRIPTION=Dating app for developers

# ===========================================
# NOTIFICATION CONFIGURATION
# ===========================================
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_VAPID_PUBLIC_KEY=your_vapid_public_key

# ===========================================
# SECURITY CONFIGURATION
# ===========================================
VITE_ENABLE_CSP=true
# Content Security Policy
VITE_SECURE_COOKIES=false
# Set to true in production with HTTPS

# ===========================================
# PERFORMANCE CONFIGURATION
# ===========================================
VITE_LAZY_LOADING=true
VITE_IMAGE_OPTIMIZATION=true
VITE_BUNDLE_ANALYZER=false

# ===========================================
# LOCALIZATION
# ===========================================
VITE_DEFAULT_LANGUAGE=en
VITE_SUPPORTED_LANGUAGES=en,hi,es,fr
VITE_ENABLE_RTL=false

# ===========================================
# TESTING CONFIGURATION
# ===========================================
VITE_ENABLE_E2E_TESTING=false
VITE_TEST_USER_EMAIL=test@devtinder.com
VITE_TEST_USER_PASSWORD=testpassword123 